{
    "compilerOptions": {
        "rootDir": "./src",
        "outDir": "./dist",
        "target": "es5",
        "lib": ["dom", "es6"],
        "module": "commonjs",
        "allowJs": true,
        "downlevelIteration": true,
        "noEmitOnError": true,
        "strict": true
    },
    "exclude": [
        "node_modules",
        "dist",
        "src/test/web-platform-tests",
    ]
}
